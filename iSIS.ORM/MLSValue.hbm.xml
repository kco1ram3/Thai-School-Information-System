<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
	<class name="iSISModel.MLSValue,iSISModel" table="MLSValue">
		<!-- Primary key-->
		<id name="ID" type="Int64" unsaved-value="0">
			<generator class="native"/>
		</id>

		<many-to-one name="Owner" class="iSISModel.MultilingualString,iSISModel" column="MLSID"/>
    <property name="LanguageCode" />
    <property name="Value" />
	</class>
</hibernate-mapping>


