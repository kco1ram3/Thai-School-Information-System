<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
  <class name="iSISModel.DiscreteGrade,iSISModel" table="DiscreteGrade">

    <!-- Primary key-->
    <id name="ID" type="Int64" unsaved-value="0">
      <generator class="native"/>
    </id>
    <property name="Symbol" />
    <property name="Point" />
    <many-to-one name="GradingSystem" column="GradingSystemID" class="iSISModel.DiscreteGradingSystem,iSISModel"/>

  </class>
</hibernate-mapping>


